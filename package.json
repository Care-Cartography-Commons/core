  "devDependencies": {
    "turbo": "^2.5.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "@eslint/js": "^9.35.0",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript-eslint": "^8.43.0"
